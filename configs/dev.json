{
    "id": "azure-devops-mermaid-viewer-dev", 
    "name": "Mermaid Viewer (dev)",
    "public": false, 
    "baseUri": "https://localhost:44300",
    "contributions": [
        {
            "id": "mermaid-renderer",
            "type": "ms.vss-code-web.content-renderer",
            "description": "Shows a mermaid preview for .mmd files",
            "targets": [
                "ms.vss-code-web.content-renderer-collection"
            ],
            "properties": {
                "name": "Daniele Extension",
                "uri": "index.html",
                "fileExtensions": [
                    "mmd", "md", "aad"
                ],
                "mimeTypes": [
                    "text/html"
                ],
                "defaultBehavior": "showRenderedContent",
                "registeredObjectId": "mermaid_viewer"
            }
        }
    ]
}