{
    "manifestVersion": 1,
    "id": "azure-devops-mermaid-viewer",
    "publisher": "DanieleCas",
    "version": "1.0.65",
    "name": "Mermaid Viewer",
    "description": "Renders a Preview for your Mermaid Diagram in your Code Repos",
    "public": true,
    "categories": ["Azure Repos"],
    "tags": [
        "mermaid",
        "viewer",
        "diagrams",
        "render",
        "markdown",
        "preview"
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/daniecas/azure-devops-mermaid-viewer"
    },
    "links": {
        "getstarted": {
            "uri": "https://github.com/daniecas/azure-devops-mermaid-viewer/tree/main#usage"
        },
        "learn": {
            "uri": "https://github.com/daniecas/azure-devops-mermaid-viewer/tree/main#run-locally"
        },
        "support": {
            "uri": "https://github.com/daniecas/azure-devops-mermaid-viewer/tree/main#for-contributors"
        },
        "issues": {
            "uri": "https://github.com/daniecas/azure-devops-mermaid-viewer/tree/main#for-contributors"
        },
        "privacypolicy": {
            "uri": "https://github.com/daniecas/azure-devops-mermaid-viewer/blob/main/Privacy.md"
        }
    },
    "icons": {
        "default": "doc/mermaid.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "content": {
        "details": {
          "path": "doc/overview.md"
        },
        "license": {
            "path": "LICENSE"
          }
    },
    "files": [
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "doc",
            "addressable": true
        }
    ]
}